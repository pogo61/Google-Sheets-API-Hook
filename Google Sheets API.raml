                </xs:sequence>
                <xs:attribute ref="gd:etag" use="required"/>
              </xs:complexType>
            </xs:element>
            <xs:element name="id" type="xs:anyURI"/>
            <xs:element name="updated" type="xs:dateTime"/>
            <xs:element name="category">
              <xs:complexType>
                <xs:attribute name="scheme" use="required" type="xs:anyURI"/>
                <xs:attribute name="term" use="required" type="xs:anyURI"/>
              </xs:complexType>
            </xs:element>
            <xs:element name="title">
              <xs:complexType>
                <xs:simpleContent>
